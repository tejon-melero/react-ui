{
  "name": "@cotidia/react-ui",
  "version": "2.3.2",
  "description": "UI Components for React",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://code.cotidia.com/cotidia/react-ui.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf lib dist",
    "build:main": "babel -d lib ./src",
    "_build:umd": "webpack --output-filename index.umd.js",
    "_build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "build": "npm-run-all --parallel build:*",
    "watch": "babel -d lib ./src --watch"
  },
  "author": {
    "name": "Guillaume Piot",
    "email": "guillaume@cotidia.com"
  },
  "contributors": [
    {
      "name": "Alex Russell",
      "email": "alex.russell@cotidia.com"
    }
  ],
  "license": "ISC",
  "files": [
    "dist",
    "lib"
  ],
  "peerDependencies": {
    "react": "^15.5 || ^16.0",
    "react-dom": "^15.5 || ^16.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.5",
    "react-dom": "^15.5",
    "rimraf": "^2.6.1",
    "webpack": "^1.14.0"
  }
}
